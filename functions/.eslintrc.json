{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended", "google"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-restricted-globals": ["error", "name", "length"],
    "prefer-arrow-callback": "error",
    "quotes": ["error", "single"],
    "max-len": ["warn", {"code": 120}]
  }
}
